algoritmo "TipoDados"

var 
nome: caractere 
idade: inteiro 
altura: real 
crianca: logico

inicio 
nome <- "Getulio"
idade <- 22 
altura <- 1.78
crianca <- falso 

escreval("Tipo de Caractere: ", nome) 
escreval("Tipo de Inteiro: ", idade)
escreval("Tipo de Real: ", altura) 
escreval("Tipo de Booleano: ", crianca)

fimalgoritmo